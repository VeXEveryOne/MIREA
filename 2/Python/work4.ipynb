    "from math import floor\n",
    "\n",
    "def main(n):\n",
    "    if n == 0:\n",
    "        return 0.62\n",
    "    elif n >= 1:\n",
    "        f_prev = main(n - 1)\n",
    "        return (\n",
    "            (f_prev - 13 * (f_prev ** 3)) / 29\n",
    "            - 1\n",
    "            - (floor(f_prev) ** 3 / 27)\n",
    "        )\n"
   ]
