   "source": [
    "from itertools import product\n",
    "\n",
    "with open('brackets_and_A_combinations.txt', 'w', encoding='utf-8') as f:\n",
    "    for length in range(1, 6):  # Длины от 1 до 5\n",
    "        for combo in product('()A', repeat=length):\n",
    "            f.write(''.join(combo) + '\\n')"
   ]
  }
